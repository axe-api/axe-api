{
  "name": "axe-api",
  "version": "0.30.0",
  "description": "AXE API is a simple tool which has been created based on Express and Knex.js to create Rest APIs quickly.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "/build"
  ],
  "directories": {
    "test": "tests"
  },
  "author": "Özgür Adem Işıklı <i.ozguradem@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "ts-node-dev --files --respawn --clear index.ts",
    "dev-kit": "ts-node-dev --respawn --clear dev-kit.ts",
    "dev-kit:install": "node ./scripts/dev-kit-install.js",
    "dev-kit:remove": "node ./scripts/dev-kit-remove.js",
    "test": "jest --runInBand",
    "test:dev": "jest --watch",
    "lint": "eslint src/**",
    "lint:watch": "esw --watch --color",
    "prepare:integration": "nodemon --ignore \"./tests/**\" ./scripts/run-integration-test.js",
    "test:postgres": "sh ./scripts/test-postgres.sh",
    "test:mysql57": "sh ./scripts/test-mysql57.sh",
    "test:mysql8": "sh ./scripts/test-mysql8.sh"
  },
  "dependencies": {
    "@types/express": "^4.17.15",
    "chalk": "^4.1.2",
    "change-case": "^4.1.2",
    "dotenv": "^14.2.0",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "knex": "^2.3.0",
    "knex-paginate": "^3.0.2",
    "knex-schema-inspector": "^2.0.4",
    "pluralize": "^8.0.0",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/accept-language-parser": "^1.5.3",
    "@types/pluralize": "^0.0.29",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unicorn": "^33.0.1",
    "eslint-watch": "^7.0.0",
    "glob": "^8.0.3",
    "jest": "^29.0.3",
    "mysql": "^2.18.1",
    "node-color-log": "^10.0.2",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "set-value": ">=4.1.0",
    "sqlite3": "^5.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
